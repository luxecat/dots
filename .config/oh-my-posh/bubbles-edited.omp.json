{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 150
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .FormattedMs }}",
          "foreground": "#282C34",
          "background": "#61AFEF",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\udb82\udcc7 ",
          "foreground": "#282C34",
          "background": "#61AFEF",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .UserName }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "julia",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf240  ",
            "charging_icon": "\u21e1 ",
            "discharging_icon": "\u21e3 "
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}",
          "foreground": "#282C34",
          "background": "#ABB2BF",
          "type": "battery",
          "style": "diamond",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#282C34{{end}}",
            "{{if eq \"Discharging\" .State.String}}#282C34{{end}}",
            "{{if eq \"Full\" .State.String}}#282C34{{end}}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "auto_upgrade": true,
  "final_space": true
}
